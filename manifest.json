{
    "id": "org.ekstep.questionunit",
    "ver": "1.0",
    "author": "Manoj Chandrashekar",
    "type": "plugin",
    "title": "Question Unit Base Plugin",
    "description": "Question Unit Base Plugin that declares the interfaces that Question Unit Plugins must define.",
    "publishedDate": "",
    "editor": {
        "main": "editor/plugin.js",
        "dependencies": [
            {
                "type": "css",
                "src": "editor/styles/styles.css"
            },
            {
                "type": "js",
                "src": "editor/js/util.js"
            },
            {
                "type": "plugin",
                "plugin": "org.ekstep.mathtext",
                "ver": "1.0"
            },
            {
                "type": "plugin",
                "plugin": "org.ekstep.libs.ckeditor",
                "ver": "1.0"
            }
        ],
        "help": {
            "src": "editor/help.md",
            "dataType": "text"
        }
    },
    "renderer": {
        "main": "renderer/plugin.js",
        "dependencies": [
            {
                "type": "js",
                "src": "renderer/components/js/base_component.js"
            },
            {
                "type": "css",
                "src": "renderer/components/css/base_component.css"
            },
            {
                "type": "js",
                "src": "renderer/components/js/question_component.js"
            },
            {
                "type": "css",
                "src": "renderer/components/css/question_component.css"
            }
        ]
    }
}